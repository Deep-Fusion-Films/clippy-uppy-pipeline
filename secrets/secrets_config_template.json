{
  "gcs": {
    "project_id": "your-gcp-project-id",
    "bucket_name": "your-default-bucket",
    "credentials_path": "/secrets/gcs_service_account.json"
  },
  "firestore": {
    "project_id": "your-gcp-project-id",
    "credentials_path": "/secrets/firestore_service_account.json"
  },
  "qwen": {
  "model_name": "Qwen1.5-7B-Chat",
  "provider": "huggingface",
  "api_key": "your-hf-token-if-needed"
  },
  "openai": {
    "api_key": "your-openai-api-key",
    "model": "whisper-1"
  },
  "embedding": {
    "provider": "openai",
    "api_key": "your-embedding-api-key",
    "model": "text-embedding-ada-002"
  },
  "transcription": {
    "model": "base",
    "language": "en"
  },
  "metadata": {
    "collection": "media_metadata",
    "schema_path": "/config/schema.json"
  },
  "logging": {
    "level": "INFO",
    "log_to_file": false,
    "log_file_path": "/logs/pipeline.log"
  }
}

